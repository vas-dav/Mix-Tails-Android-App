<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mix_tailsapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1migi2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo1migi2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.FullScreen">
        <activity android:name=".DrinkRecommendationPage"></activity>
        <activity
            android:name=".SignupConfirmationScreen"
            android:parentActivityName=".SignupActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".AppLaunching" />
        <activity android:name=".QuestionSpinner" />
        <activity android:name=".ChosenDrinkSecondActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>